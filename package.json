{
    "name": "widget-template-vue",
    "version": "1.0.0",
    "description": "Widget Template using Vue.js",
    "main": "index.html",
    "config": {
        "publicPath": null
    },
    "scripts": {
        "test": "mocha test/e2e/bootstrap.js --recursive test --timeout 10000",
        "debugTest": "mocha --inspect-brk test/bootstrap.js --recursive test --timeout 10000",
        "build": "webpack --config ./webpack/webpack.config.prod.js",
        "start": "webpack-dev-server --config ./webpack/webpack.config.dev.js --open",
        "startS3": "webpack-dev-server --config ./webpack/webpack.config.dev.s3.js",
        "debugS3": "node --inspect node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack/webpack.config.dev.s3.js",
        "lint": "eslint src/**/*.js",
        "lintFix": "eslint src/**/*.js --fix",
        "lintMark": "markdownlint --config .markdownlint.json --ignore ./node_modules/** ./**/*.md",
        "lintMarkFix": "markdownlint --fix --config .markdownlint.json --ignore ./node_modules/** ./**/*.md"
    },
    "repository": {
        "type": "git",
        "url": "git@itgit.dsone.3ds.com:widget-lab/widget-template-vue.git"
    },
    "author": "widget-lab",
    "license": "MIT",
    "devDependencies": {
        "@auth0/s3": "1.0.0",
        "@babel/core": "7.5.5",
        "@babel/preset-env": "7.5.5",
        "@mdi/font": "3.8.95",
        "babel-loader": "8.0.6",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "5.0.4",
        "css-loader": "3.1.0",
        "deepmerge": "4.0.0",
        "eslint": "5.16.0",
        "eslint-config-prettier": "6.0.0",
        "eslint-config-vuetify": "0.4.0",
        "eslint-loader": "2.2.1",
        "eslint-plugin-html": "6.0.0",
        "eslint-plugin-json": "1.4.0",
        "eslint-plugin-mocha": "6.0.0",
        "eslint-plugin-vue": "5.2.3",
        "express": "4.17.1",
        "fibers": "4.0.1",
        "file-loader": "4.1.0",
        "highlight.js": "9.15.9",
        "markdownlint": "0.17.0",
        "markdownlint-cli": "0.18.0",
        "material-design-icons-iconfont": "5.0.1",
        "mermaid": "8.3.1",
        "mocha": "6.2.0",
        "puppeteer": "1.19.0",
        "raw-loader": "3.1.0",
        "remarkable": "2.0.0",
        "sass": "1.22.9",
        "sass-loader": "7.3.1",
        "style-loader": "0.23.1",
        "vue": "2.6.10",
        "vue-loader": "15.7.1",
        "vue-template-compiler": "2.6.10",
        "vuetify": "2.0.11",
        "vuetify-loader": "1.3.0",
